{
  "name": "ocr-studio-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:ocr-studio-ui": "node dist/ocr-studio-ui/server/server.mjs",
    "generate:api": "del-cli --force ./src/app/core/api/ src/app/core/api/model && openapi-generator-cli generate -i ./src/assets/openapi.json -g typescript-angular -o ./src/app/core/modules/openapi --additional-properties fileNaming=kebab-case --generate-alias-as-model"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^18.2.3",
    "@angular/common": "^18.2.0",
